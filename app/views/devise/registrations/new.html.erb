<h2>Sign up</h2>

<%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
  <div class="form-group"><%= f.input :first_name %> </div>

  <div class="form-group"><%= f.input :last_name %> </div>

  <div class="form-group"><%= f.input :profile_name %> </div>

  <div class="form-group"><%= f.input :email %></div>

  <div class="form-group"><%= f.input :password %></div>

  <div class="form-group"><%= f.input :password_confirmation %></div>

  <div class="form-group"><%= f.button :submit %></div>
<% end %>

<%= render "devise/shared/links" %>
